<p id="notice"><%= notice %></p>

<h1>Events People Report</h1>

<table>
  <thead>
    <tr>
      <th>Personal ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Event Role</th>
      <th>Hours Worked</th>
      <th>Organization Name</th>
      <th>Sub-Organization Name</th>
      <th>Email Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report.each do |report| %>
      <tr>
        <td><%= report.PersonalID %></td>
        <td><%= report.FirstName %></td>
        <td><%= report.LastName %></td>
        <td><%= report.EventRole %></td>
        <td><%= report.HoursWorked %></td>
        <td><%= report.OrganizationName %></td>
        <td><%= report.SubOrganizationName %></td>
        <td><%= report.EmailAddress %></td>
        <td><%= link_to 'Show', events_person %></td>
        <td><%= link_to 'Edit', edit_events_person_path(events_person) %></td>
        <td><%= link_to 'Remove', events_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>